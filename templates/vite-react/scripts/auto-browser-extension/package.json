{
  "name": "phion-dev-tools",
  "displayName": "Phion Dev Tools",
  "description": "Tools for Phion development: auto-open browser, optimize workspace, monitor server, clear ports",
  "version": "0.0.5",
  "publisher": "phion",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "phion.startProject",
        "title": "Phion: Start Project"
      },
      {
        "command": "phion.openPreview",
        "title": "Phion: Open Preview"
      },
      {
        "command": "phion.clearPortsAndOpen",
        "title": "Phion: Fix Connection and Open Preview"
      },
      {
        "command": "phion.fixConnection",
        "title": "Phion: Fix Connection Issues"
      },
      {
        "command": "phion.resetProjectHistory",
        "title": "Phion: Reset Project History (for testing)"
      }
    ]
  },
  "scripts": {},
  "devDependencies": {}
}
